{
  "mcpServers": {
    "mysql-database": {
      "command": "node",
      "args": ["/path/to/mav-mysql-mcp-server/build/index.js"],
      "env": {
        "MYSQL_HOST": "localhost",
        "MYSQL_PORT": "3306",
        "MYSQL_USER": "your_mysql_user",
        "MYSQL_PASSWORD": "your_mysql_password",
        "MYSQL_DATABASE": "your_database_name",
        "ALLOW_WRITE_OPERATIONS": "false",
        "CONNECTION_LIMIT": "5",
        "QUERY_TIMEOUT": "30000",
        "MAX_RESULTS": "1000",
        "MCP_DEBUG": "false"
      }
    }
  }
}